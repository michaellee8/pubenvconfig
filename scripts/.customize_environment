# sudo apt-get update -yq

# sudo apt-get -t unstable install -yq vim=2:8.2.0716-3 python3.8 libpython3.8-dev

# sudo apt-get install -t unstable install -yq neovim=0.4.4-1 gcc

sudo apt-get install -yq vifm

pip3 install pynvim

sudo dpkg -i $HOME/gh_1.2.1_linux_amd64.deb && sudo apt-get install -f

sudo apt-get install -yq universal-ctags

~/pubenvconfig/scripts/install-protobuf.sh

sudo apt-get update -yq
sudo apt-get install -yq apt-transport-https
sudo sh -c 'wget -qO- https://dl-ssl.google.com/linux/linux_signing_key.pub | apt-key add -'
sudo sh -c 'wget -qO- https://storage.googleapis.com/download.dartlang.org/linux/debian/dart_stable.list > /etc/apt/sources.list.d/dart_stable.list'

sudo apt-get update -yq
sudo apt-get install -yq dart

sudo update-java-alternatives -s java-1.8.0-openjdk-amd64 && export JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64

